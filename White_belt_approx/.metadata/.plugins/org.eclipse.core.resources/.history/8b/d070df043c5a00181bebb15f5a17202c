#include <vector>
#include <string>
#include <iostream>
#include <map>
using namespace std;



class TreeNode {
public:
	TreeNode () {
		value = '#';
	}
	void AddWord(string& word) {
		if (word.size() > 1) {
			value = word[0];
			word.erase(word.begin());
			children[word[0]].AddWord(word);
		}

	}
private:
	char value;
	map<char, TreeNode> children;
};


