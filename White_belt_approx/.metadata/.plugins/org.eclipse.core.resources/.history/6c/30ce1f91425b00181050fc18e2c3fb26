/*
 * 3 1 1 2
 *
 *
 */

#include <vector>
#include <iostream>
#include <set>
using namespace std;

void PrintS(const set<int>& s) {
	for (const auto& i : s) {
		cout << i << " ";
	}
}

void PrintG(const vector<set<int>>& v) {
	for (int i = 1; i <= v.size(); i++ ) {
		cout << "Node: " << i << " linked with nodes: ";
		PrintS(v[i]);
		cout << endl;
	}
}

int main() {
	int num_nods;
	int num_links;
	cin >> num_nods >> num_links;
	vector<set<int>> graph(num_nods);

	for (int i = 0; i < num_links; i++) {
		int node1;
		int node2;
		cin >> node1 >> node2;
		graph[node1 - 1].insert(node2);
		graph[node2 - 1].insert(node1);
	}
	cout << "Readed" << endl;

	cout << endl;
	PrintG(graph);
	return 0;
}
