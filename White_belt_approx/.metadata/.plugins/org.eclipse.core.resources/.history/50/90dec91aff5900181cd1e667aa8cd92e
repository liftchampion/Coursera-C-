#include <iostream>
using namespace std;

int Factorial(int n) {
	int result = 1;
	if (n == 1 || n == 0) {
		return 1;
	}
	else {
		while (n > 1) {
			result *= n;
			n--;
		}
	}
	return result;
}

int main() {
	int d14 = 0;
	int d7 = 0;
	int d2 = 0;
	int n;
	cin >> n;
	for (int i = 0; i < n; ++i) {
		int temp;
		cin >> temp;
		if (temp % 14 == 0) {
			d14++;
		}
		else if (temp % 7 == 0) {
			d7++;
		}
		else if (temp % 2 == 0) {
			d2++;
		}
	}
	cout << Factorial(n) - d14 * n - d7 * d2;
	return 0;
}
