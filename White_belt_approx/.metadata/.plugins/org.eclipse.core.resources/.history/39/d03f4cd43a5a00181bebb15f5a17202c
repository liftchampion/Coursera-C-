#include <vector>
#include <string>
#include <iostream>
#include <map>
using namespace std;



class TreeNode {
public:
	TreeNode () {
		value = "#";
	}
	int AddWord(string& word) {
		value = word[0];
		word.erase(word.begin());
	}
private:
	char value;
	map<char, TreeNode> children;
};


class Forest {
public:
	int AddWord(string& word) {
		char letter = word[0];
		word.erase(word.begin());
		return trees[letter].AddWord(word);
	}
private:
	map<char, TreeNode> trees;
};



int main() {
	string word;
	cin >> word;
	return 0;
}
