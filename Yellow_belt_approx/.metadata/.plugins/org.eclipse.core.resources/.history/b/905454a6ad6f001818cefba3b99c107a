#include <vector>
#include <iostream>
#include <stack>
using namespace std;

void PrintVector(const vector<int>& v) {
	for (const auto& i : v) {
		cout << i << " ";
	}
}

template <class Iterator>
void SetMid(stack<int>& nums, Iterator seq_begin, Iterator seq_end) {
	auto mid_it = seq_begin + (seq_end - 1 - seq_begin) / 2;
	*mid_it = nums.top();
	nums.pop();
}


int main() {
	unsigned n;
	cin >> n;
	stack<int> nums;
	for (unsigned i = 1; i <= n; i++) {
		nums.push(i);
	}
	vector<int> sequence(n);
	SetMid(nums, sequence.begin(), sequence.end());

	PrintVector(sequence);

	return 0;
}
